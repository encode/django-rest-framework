msgid "Filters"
msgstr "Filtres"

#: templates/rest_framework/filters/django_filter.html:2
#: templates/rest_framework/filters/django_filter_crispyforms.html:4
msgid "Field filters"
msgstr "Filtres de champ"

#: templates/rest_framework/filters/ordering.html:3
msgid "Ordering"
msgstr "Ordre"

